{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "npx prisma generate && next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "dotenv -e .env.local -- prisma generate",
    "db:push": "dotenv -e .env.local -- prisma db push --force-reset",
    "db:migrate": "dotenv -e .env.local -- prisma migrate dev",
    "db:seed": "dotenv -e .env.local -- tsx prisma/seed.ts",
    "db:seed-users": "dotenv -e .env.local -- tsx prisma/seed_users.ts",
    "db:seed-tables": "dotenv -e .env.local -- tsx prisma/seed_tables.ts",
    "db:seed-reservations": "dotenv -e .env.local -- tsx prisma/seed_reservations.ts",
    "db:studio": "prisma studio"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
